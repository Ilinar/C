#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main()
{
    int a,b,c;
    printf("a: \n");
    scanf("%d",&a);
    printf("b: \n");
    scanf("%d",&b);
    c=a+b;
    if (c>10) {
            printf("The sum of your number is larger than 10.");
    }
    else{
        printf("The sum of your number is not larger than 10.");
    }
    return 0;
}
